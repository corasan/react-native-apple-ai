{
  "name": "react-native-foundation-models",
  "version": "0.1.0",
  "description": "A React Native Nitro module",
  "main": "./lib/commonjs/index.js",
  "module": "./lib/module/index.js",
  "types": "./lib/typescript/src/index.d.ts",
  "react-native": "./src/index",
  "source": "./src/index",
  "bin": {
    "generable": "./bin/generable"
  },
  "author": "Developer",
  "scripts": {
    "postinstall": "tsc || exit 0;",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf android/build node_modules/**/android/build lib",
    "typescript": "tsc",
    "build": "rm -rf lib && bun typecheck && bob build",
    "specs": "bun nitro-codegen --logLevel=\"debug\"",
    "generate": "node lib/commonjs/cli.js",
    "generate:dev": "ts-node src/cli.ts"
  },
  "keywords": [
    "react-native",
    "nitro"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Developer/foundation-models.git"
  },
  "license": "MIT",
  "bugs": "https://github.com/Developer/foundation-models/issues",
  "homepage": "https://github.com/Developer/foundation-models#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "provenance": true
  },
  "files": [
    "src",
    "lib",
    "nitrogen",
    "cpp",
    "ios",
    "plugin",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "react-native.config.js",
    "android/build.gradle",
    "android/gradle.properties",
    "android/CMakeLists.txt",
    "android/src",
    "app.plugin.js",
    "*.podspec",
    "generable.config.example.ts",
    "bin",
    "README.md"
  ],
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-nitro-modules": "*"
  },
  "devDependencies": {
    "@expo/config-plugins": "^8.0.0",
    "@types/react": "*",
    "nitro-codegen": "^0.26.2",
    "react-native-builder-bob": "^0.40.12",
    "ts-node": "^10.9.0"
  },
  "codegenConfig": {
    "name": "FoundationModelsSpec",
    "type": "modules",
    "jsSrcsDir": "src"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "commonjs",
        {
          "esm": true
        }
      ],
      [
        "module",
        {
          "esm": true
        }
      ],
      [
        "typescript",
        {
          "esm": true
        }
      ]
    ]
  }
}
